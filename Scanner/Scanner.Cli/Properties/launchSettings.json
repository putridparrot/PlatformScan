    "Scanner (run all)": {
      "commandName": "Project",
      "commandLineArgs": "run",
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Development"
      }
    },
    "Scanner (List plugins)": {
      "commandName": "Project",
      "commandLineArgs": "list",
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Development"
      }
