        "DOTNET_ENVIRONMENT": "Development"
      }
    },
    "Scanner (HTTP Headers)": {
      "commandName": "Project",
      "commandLineArgs": "run --plugin http-headers",
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Development"
      }
    },
    "Scanner (CVE)": {
      "commandName": "Project",
      "commandLineArgs": "run --plugin cve",
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Development"
      }
    }
  }
}